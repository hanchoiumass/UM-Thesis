	pmid = {19017133},
	keywords = {Carbon Dioxide, Cell Respiration, Ecosystem, Plant Roots, Temperature, Trees},
	pages = {1467--1483},
	file = {Submitted Version:C\:\\Users\\hanna\\Zotero\\storage\\VBF3VEFY\\Burton et al. - 2008 - Adjustment of forest ecosystem root respiration as.pdf:application/pdf},
}

