	keywords = {Genotype, Plant Leaves, Biomarkers, Metabolomics, Stress, Physiological, Gene Expression Regulation, Plant, Droughts, Helianthus, Maintainer–restorer lines, Metabolic markers, Sunflower, Water stress},
	pages = {56},
