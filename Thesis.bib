	keywords = {Animals, Female, Male, Metabolome, Behavior, Animal, Ecosystem, Metabolic Networks and Pathways, Rivers, Spain, Swimming, Trout},
	pages = {e0181697},
	file = {Full Text:C\:\\Users\\hanna\\Zotero\\storage\\W2U4LINR\\Oromi et al. - 2017 - Differential metabolic profiles associated to move.pdf:application/pdf;Full Text:C\:\\Users\\hanna\\Zotero\\storage\\WVD8ML4L\\Oromi et al. - 2017 - Differential metabolic profiles associated to move.pdf:application/pdf},
